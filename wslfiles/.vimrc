syntax on

set tabstop=4
set noerrorbells
set smartindent
set rnu
set smartcase
set noswapfile
set undodir=~/.vim/undodir
set undofile
set incsearch

"to start vim in insert mode
"by bypassing the ambiguous
"utf-8 characters and by not
"requesting cursor position
set t_u7=
